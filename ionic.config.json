{
  "name": "staticTodo",
  "integrations": {
    "cordova": {}
  },
  "type": "ionic1",
  "watchPatterns": [
    "scss/**/*",
    "www/**/*",
    "!www/lib/**/*",
    "!www/**/*.map"
  ],
  "proxies": [
    {
      "path": "/login",
      "proxyUrl": "http://localhost:3000/login"
    },
    {
      "path": "/existemail",
      "proxyUrl": "http://localhost:3000/existemail"
    },
    {
      "path": "/existnickname",
      "proxyUrl": "http://localhost:3000/existnickname"
    },
    {
      "path": "/adduser",
      "proxyUrl": "http://localhost:3000/adduser"
    },
    {
      "path": "/getMyself",
      "proxyUrl": "http://localhost:3000/getMyself"
    },
    {
      "path": "/getTasksGroup",
      "proxyUrl": "http://localhost:3000/getTasksGroup"
    },
    {
      "path": "/getTasksGroup/partage",
      "proxyUrl": "http://localhost:3000/getTasksGroup/partage"
    },
    {
      "path": "/addTasksGroup",
      "proxyUrl": "http://localhost:3000/addTasksGroup"
    },
    {
      "path": "/deleteTasksGroup",
      "proxyUrl": "http://localhost:3000/deleteTasksGroup"
    },
    {
      "path": "/updateProject",
      "proxyUrl": "http://localhost:3000/updateProject"
    },
    {
      "path": "/partageProjet",
      "proxyUrl": "http://localhost:3000/partageProjet"
    },
    {
      "path": "/partageProjet/remove",
      "proxyUrl": "http://localhost:3000/partageProjet/remove"
    },
    {
      "path": "/getTaskSet",
      "proxyUrl": "http://localhost:3000/getTaskSet"
    },
    {
      "path": "/addTask",
      "proxyUrl": "http://localhost:3000/addTask"
    },
    {
      "path": "/deleteTaskSet",
      "proxyUrl": "http://localhost:3000/deleteTaskSet"
    },
    {
      "path": "/updateTaskSet",
      "proxyUrl": "http://localhost:3000/updateTaskSet"
    }
  ]
}
